215                 \}\par
216                 else\par
217                 \{\par
218                     if (countValorActual == countValorMasFrecuente)\par
219                     \{\par
220                         moda.Add(valorActual);\par
221                     \}\par
222                     else if (countValorActual > countValorMasFrecuente)\par
223                     \{\par
224                         countValorMasFrecuente = countValorActual;\par
225                         moda.Clear();\par
226                         moda.Add(valorActual);\par
227                     \}\par
228                     if (!last) valorActual = valor;\par
229                     countValorActual = 1;\par
230                 \}\par
231             \}\par
